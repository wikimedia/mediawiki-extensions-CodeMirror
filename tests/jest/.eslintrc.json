{
	"extends": [
		"wikimedia/jest",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "module"
	},
	"globals": {
		"mockMwConfigGet": true
	},
	"env": {
		"browser": true,
		"jest": true
	},
	"rules": {
		"jest/expect-expect": [ "error", {
			"assertFunctionNames": [ "expect", "test" ]
		} ],
		"jest/valid-title": [ "error", {
			"ignoreTypeOfDescribeName": true,
			"ignoreTypeOfTestName": true
		} ]
	}
}
